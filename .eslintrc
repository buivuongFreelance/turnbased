{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "no-unused-vars": ["error", { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }],
        "react/prop-types": 0
    },
    "extends": [
        "plugin:react/recommended",
    ],
    "plugins": [
    ],
    "settings": {
        "react": {
            "version": "latest"
        }
    }
}
